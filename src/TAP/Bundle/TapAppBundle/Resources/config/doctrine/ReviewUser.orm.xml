<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TAP\Bundle\TapAppBundle\Entity\ReviewUser" table="review_user">
    <id name="reviewKey" type="integer" column="review_key">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="status" type="boolean" column="status" nullable="true"/>
    <field name="title" type="text" column="title" nullable="true"/>
    <field name="providerUserKey" type="integer" column="provider_user_key" nullable="true"/>
    <field name="consumerUserKey" type="integer" column="consumer_user_key" nullable="true"/>
    <field name="reviewCriterial1" type="string" column="review_criterial_1" length="255" nullable="true"/>
    <field name="reviewCriterial2" type="string" column="review_criterial_2" length="255" nullable="true"/>
    <field name="reviewCriterial3" type="string" column="review_criterial_3" length="255" nullable="true"/>
    <field name="reviewCriterial4" type="string" column="review_criterial_4" length="255" nullable="true"/>
    <field name="comment" type="text" column="comment" nullable="true"/>
    <field name="createTime" type="datetime" column="create_time" nullable="true"/>
    <field name="updateTime" type="datetime" column="update_time" nullable="true"/>
    <field name="deleteTime" type="datetime" column="delete_time" nullable="true"/>
  </entity>
</doctrine-mapping>
